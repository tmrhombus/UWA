#!/bin/sh


preselection='pt1>24&&pt2>20&&vertices>0&&diLeptons==0&&lPFIsoDB<0.1&&tauLooseMVAIso>0'
oswselection='charge==0&&mt1>70'
ossignalselection='charge==0&mt1<40'
sswselection='charge!=0&&mt1>70'
sssignalselection='charge!=0&&mt1<40'
qcdselection='pt1>24&&pt2>20&&vertices>0&&diLeptons==0&&lPFIsoDB<0.3&&tauLooseMVAIso>0&&charge!=0&&mt1<40'
relaxedSelection='pt1>24&&pt2>20&&mt1<40'
vbfrelaxedSelection='pt1>24&&pt2>20&&vertices>0&&lPFIsoDB<0.5&charge==0&&mt1<40'
vbfSelection0='(nJetsPt30==0)'
vbfSelection1='(nJetsPt30>=1&&nJetsBTagCSVMPt20==0&&(!(nJetsPt30>=2&&vbfNJetsGap30==0&&vbfMVA>0.5)))' 
vbfSelection2='(nJetsPt30>=2&&vbfNJetsGap30==0&&vbfMVA>0.5)'
luminosity=5010.

mkdir eleTauPlots



MakeDataCard channel='eleTau'    dataFile='../inputs/ltau/DATA.root' zEmbeddedSample='../inputs/ltau/ETEM.root' wThreeJetsFile='../inputs/ltau/W3JETS.root'  zttFile='../inputs/ltau/ZJETS.root' zllFile='../inputs/ltau/ZJETS.root' wFile='../inputs//ltau/W.root' vvFile='../inputs/ltau/VV.root' topFile='../inputs/ltau/TOP.root' preselection=$preselection       oswselection=$oswselection	       ossignalselection=$ossignalselection         sswselection=$sswselection	      sssignalselection=$sssignalselection  qcdselection=$qcdselection  luminosityErr=0.045 qcdfactor=1.07 qcdfactorErr=0.05 vvErr=0.3  zttScale=0.976 zttScaleErr=0.033    wFactorErr=0.06    zjfterr=0.1  zlfterr=0.24   topErr=0.075 	muID=0.0 muIDErr=0.0 eleID=1.00 eleIDErr=0.01 tauID=1.0 tauIDErr=0.06  dir='../inputs/ltau/'  bID=0.976  bIDErr=0.10  bMisID=1.21  bMisIDErr=0.17    bitmask=1,0,0,0  vbfSelection0=$vbfSelection0 vbfSelection1=$vbfSelection1  vbfSelection2=$vbfSelection2   relaxedSelection=$relaxedSelection  weight='__WEIGHT__*__CORR__'  luminosity=$luminosity  zlftFactor=1.0 variable='mass' bins=30 min=0.0 max=300 outputFile='eleTauPlots/eleTau_MVis.root'
  
MakeDataCard channel='eleTau'    dataFile='../inputs/ltau/DATA.root' zEmbeddedSample='../inputs/ltau/ETEM.root'  wThreeJetsFile='../inputs/ltau/W3JETS.root'  zttFile='../inputs/ltau/ZJETS.root' zllFile='../inputs/ltau/ZJETS.root' wFile='../inputs//ltau/W.root' vvFile='../inputs/ltau/VV.root' topFile='../inputs/ltau/TOP.root' preselection=$preselection       oswselection=$oswselection	       ossignalselection=$ossignalselection         sswselection=$sswselection	      sssignalselection=$sssignalselection  qcdselection=$qcdselection  luminosityErr=0.045 qcdfactor=1.07 qcdfactorErr=0.05 vvErr=0.3  zttScale=0.976 zttScaleErr=0.033    wFactorErr=0.06    zjfterr=0.1  zlfterr=0.24   topErr=0.075 	muID=0.0 muIDErr=0.0 eleID=1.00 eleIDErr=0.01 tauID=1.0 tauIDErr=0.07  dir='../inputs/ltau/'  bID=0.976  bIDErr=0.10  bMisID=1.21  bMisIDErr=0.17    bitmask=1,0,0,0  vbfSelection0=$vbfSelection0 vbfSelection1=$vbfSelection1  vbfSelection2=$vbfSelection2   relaxedSelection=$relaxedSelection  weight='__WEIGHT__*__CORR__'  luminosity=$luminosity  zlftFactor=1.0 variable='svMass' bins=30 min=0.0 max=300 outputFile='eleTauPlots/eleTau_SVFit.root'
 
